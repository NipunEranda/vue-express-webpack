{
  "name": "agilehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" ",
    "dev:client": "vue-cli-service serve --port 8888",
    "dev:server": "cross-env NODE_ENV=production nodemon server/index.js",
    "build": "rm -rf dist && npm run build:client && npm run build:server && npm run generate:packagejson",
    "build:client": "vue-cli-service build",
    "build:server": "webpack --mode production",
    "generate:packagejson": "echo '{ \"scripts\": { \"start\": \"node index.js\" } }' >> dev_package.json && mv dev_package.json dist/package.json",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "core-js": "^3.8.3",
    "express": "^4.19.2",
    "mongoose": "^8.5.4",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.1.4",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "~4.5.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
